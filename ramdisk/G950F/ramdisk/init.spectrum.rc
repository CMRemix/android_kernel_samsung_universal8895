# SPECTRUM KERNEL MANAGER
# Ramdisk file for profile based kernel management
# Implimentation inspired by Franco's fku profiles

# Initialization
on property:sys.boot_completed=1
   # Set default profile on first boot
   exec u:r:init:s0 root root -- /init.spectrum.sh
   exec u:r:su:s0 root root -- /init.spectrum.sh
   # Enable Spectrum support
   setprop spectrum.support 1
   # Add kernel name
   setprop persist.spectrum.kernel ZION959-Kernel

# Balance (default profile)
on property:persist.spectrum.profile=0
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor interactive
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 455000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1690000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor interactive
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 741000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 2314000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/boost 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/boostpulse_duration 40000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/hispeed_freq 858000
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/boost 0
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/boostpulse_duration 40000
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/hispeed_freq 1248000
    write /dev/cpuset/foreground/cpus 0-2,4-7
    write /dev/cpuset/foreground/boost/cpus 4-7
    write /dev/cpuset/top-app/cpus 0-7

# Performance (based on DrunkSauce Lite)
on property:persist.spectrum.profile=1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor schedutil
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 598000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1690000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor schedutil
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq_hardlimit 2704000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 1066000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 2704000
    write /dev/cpuset/foreground/cpus 0-2,4-7
    write /dev/cpuset/foreground/boost/cpus 4-7
    write /dev/cpuset/top-app/cpus 0-7

# Battery
on property:persist.spectrum.profile=2
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor zzmoove
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 455000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1248000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor zzmoove
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 741000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 1937000
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/afs_threshold1 25
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/afs_threshold2 50
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/afs_threshold3 75
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/afs_threshold4 90
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/down_threshold 40
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/fast_scaling_down 0
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/fast_scaling_up 0
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/ignore_nice_load 0
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/sampling_down_factor 1
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/sampling_rate 200000
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/sampling_rate_min 200000
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/scaling_proportional 0
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/smooth_up 75
    write /sys/devices/system/cpu/cpu0/cpufreq/zzmoove/up_threshold 80
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/afs_threshold1 25
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/afs_threshold2 50
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/afs_threshold3 75
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/afs_threshold4 90
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/down_threshold 40
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/fast_scaling_down 0
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/fast_scaling_up 0
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/ignore_nice_load 0
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/sampling_down_factor 1
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/sampling_rate 200000
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/sampling_rate_min 200000
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/scaling_proportional 0
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/smooth_up 75
    write /sys/devices/system/cpu/cpu4/cpufreq/zzmoove/up_threshold 80
    write /dev/cpuset/foreground/cpus 0-2,4-5
    write /dev/cpuset/foreground/boost/cpus 4-5
    write /dev/cpuset/top-app/cpus 0-5

# Gaming
on property:persist.spectrum.profile=3
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor ondemand
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 1053000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1690000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor ondemand
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 1703000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 2314000
    write /sys/devices/system/cpu/cpu0/cpufreq/ondemand/sampling_down_factor 1
    write /sys/devices/system/cpu/cpu0/cpufreq/ondemand/sampling_rate 1000000
    write /sys/devices/system/cpu/cpu0/cpufreq/ondemand/up_threshold 95
    write /sys/devices/system/cpu/cpu4/cpufreq/ondemand/sampling_down_factor 1
    write /sys/devices/system/cpu/cpu4/cpufreq/ondemand/sampling_rate 1000000
    write /sys/devices/system/cpu/cpu4/cpufreq/ondemand/up_threshold 95
    write /dev/cpuset/foreground/cpus 0-2,4-7
    write /dev/cpuset/foreground/boost/cpus 4-7
    write /dev/cpuset/top-app/cpus 0-7
